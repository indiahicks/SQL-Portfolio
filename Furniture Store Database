CREATE TABLE furniture_store_inventory (id INTEGER PRIMARY KEY, name TEXT, quanity INTEGER, price INTEGER, aisle INTEGER);

INSERT INTO furniture_store_inventory VALUES (1, "Sectional Sofa", 100, 1243,1);
INSERT INTO furniture_store_inventory VALUES (2, "Coffee Table", 50, 400,1);
INSERT INTO furniture_store_inventory VALUES (3, "Bar Stool", 23, 215,1);
INSERT INTO furniture_store_inventory VALUES (4, "Dining Table", 100, 111,2);
INSERT INTO furniture_store_inventory VALUES (5, "King Beds", 67, 1300,3);

SELECT name, aisle 
FROM furniture_store_inventory 
ORDER BY price;
